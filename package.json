    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "ethers": "^5.0.5",
    "lerna": "^5.0.0",
    "nyc": "^15.1.0",
